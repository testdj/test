          <camunda:taskListener event="complete">
            <camunda:script scriptFormat="Groovy">oneRecezent=task.getExecution().getVariable("oneRecezent");
taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());

formFields = taskFormData.getFormFields();
if(formFields!=null){
   for(field in formFields){
       if( field.getId() == "selRecezentiPon"){
            oneRecezent=field.getValue().getValue();
       }
   }
}
task.getExecution().setVariable("oneRecezent",oneRecezent);</camunda:script>
